{
  "arrowParens": "always",
  "printWidth": 80,
  "singleQuote": true,
  "semi": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@cubik/(.*)$",
    "^cubik/(.*)$",
    "",
    "^~/(.*)$",
    "^[./]"
  ]
}
